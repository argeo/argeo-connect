<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:util="http://www.springframework.org/schema/util" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
		http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
		http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-2.5.xsd">

	<!-- Resources data for the model -->
	<util:map id="initResources">
		<entry key="Countries"
			value="osgibundle:/resources/catalogs/csvFiles/ISO3166-1-countries.csv" />
		<entry key="Languages"
			value="osgibundle:/resources/catalogs/csvFiles/ISO639-1-languages.csv" />
	</util:map>

	<!-- Maps the CSV files used to initialise the system with dummy business 
		data -->
	<util:map id="demoBusinessData">
		<entry key="groupFile"
			value="osgibundle:/resources/businessData/csvFiles/demo-goups.csv" />
		<entry key="userFile"
			value="osgibundle:/resources/businessData/csvFiles/demo-users.csv" />
		<entry key="orgFile"
			value="osgibundle:/resources/businessData/csvFiles/demo-orgs.csv" />
		<entry key="personFile"
			value="osgibundle:/resources/businessData/csvFiles/demo-persons.csv" />
		<entry key="demoImages" value="osgibundle:/resources/businessData/images" />
		<entry key="demoLogo"
			value="osgibundle:/resources/businessData/images/logoSmall.png" />
	</util:map>
</beans>