<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="openNetworkEditor" class="org.argeo.connect.demo.gr.ui.commands.OpenNetworkEditor"
		scope="prototype">
	</bean>
	<bean id="generateSiteReport"
		class="org.argeo.connect.demo.gr.ui.commands.GenerateSiteReport"
		scope="prototype">
		<property name="repository">
			<bean factory-bean="grBackend" factory-method="getRepository" />
		</property>
	</bean>
	<bean id="refreshNetworkBrowserView"
		class="org.argeo.connect.demo.gr.ui.commands.RefreshNetworkBrowserView"
		scope="prototype">
	</bean>
	<bean id="openSiteEditor" class="org.argeo.connect.demo.gr.ui.commands.OpenSiteEditor"
		scope="prototype">
	</bean>
	<bean id="createNetwork" class="org.argeo.connect.demo.gr.ui.commands.CreateNetwork"
		scope="prototype">
	</bean>
	<bean id="createSite" class="org.argeo.connect.demo.gr.ui.commands.CreateSite"
		scope="prototype">
		<property name="repository">
			<bean factory-bean="grBackend" factory-method="getRepository" />
		</property>
	</bean>
</beans>
