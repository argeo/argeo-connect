<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:util="http://www.springframework.org/schema/util" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
	http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
	http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-2.5.xsd">

	<!-- Roles used in the user interface -->
	<util:map id="managedRoles">
		<entry>
			<key>
				<util:constant static-field="org.argeo.connect.demo.gr.GrConstants.ROLE_CONSULTANT" />
			</key>
			<value>ROLE_CONSULTANT</value>
		</entry>
		<entry>
			<key>
				<util:constant static-field="org.argeo.connect.demo.gr.GrConstants.ROLE_MANAGER" />
			</key>
			<value>ROLE_MANAGER</value>
		</entry>
		<entry>
			<key>
				<util:constant static-field="org.argeo.connect.demo.gr.GrConstants.ROLE_ADMIN" />
			</key>
			<value>ROLE_ADMIN</value>
		</entry>
		<!-- <entry> <key> <util:constant static-field="com.agfa.sebi.SebiConstants.ROLE_READ_ONLY" 
			/> </key> <value>ROLE_READ_ONLY</value> </entry> -->
	</util:map>

	<!-- Various site types -->
	<util:list id="siteTypes" scope="prototype">
		<util:constant static-field="org.argeo.connect.demo.gr.GrConstants.NATIONAL" />
		<util:constant static-field="org.argeo.connect.demo.gr.GrConstants.BASE" />
		<util:constant static-field="org.argeo.connect.demo.gr.GrConstants.NORMAL" />
	</util:list>

	<!-- Various point types -->
	<util:list id="pointTypes" scope="prototype">
		<value>main</value>
	</util:list>
</beans>
