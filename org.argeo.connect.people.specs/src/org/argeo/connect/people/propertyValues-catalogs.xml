<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:util="http://www.springframework.org/schema/util" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
	http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
	http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-2.5.xsd">

	<!-- Maintain here the predefined possible values given a property name-->
	<util:map id="people.businessCatalogs">
		<entry value-ref="PEOPLE_CONTACT_CATEGORY">
			<key>
				<util:constant static-field="org.argeo.connect.people.PeopleNames.PEOPLE_CONTACT_CATEGORY" />
			</key>
		</entry>
	</util:map>
	

	<!-- Contact categories: phone, mail...  -->
	<util:map id="PEOPLE_CONTACT_CATEGORY">
		<entry>
			<key>
				<util:constant static-field="org.argeo.connect.people.PeopleTypes.PEOPLE_PHONE" />
			</key>
			<value>Phone</value>
		</entry>
	</util:map>
</beans>