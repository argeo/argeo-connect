<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- Entity References CRUD -->
	<bean id="org.argeo.connect.people.ui.addEntityReferences"
		class="org.argeo.connect.people.ui.commands.AddEntityReferences"
		scope="prototype">
		<property name="peopleService" ref="peopleService" />
	</bean>
	<bean id="org.argeo.connect.people.ui.addEntityReferenceWithPosition"
		class="org.argeo.connect.people.ui.commands.AddEntityReferenceWithPosition"
		scope="prototype">
		<property name="peopleService" ref="peopleService" />
	</bean>
	<bean id="org.argeo.connect.people.ui.editEntityReferenceWithPosition"
		class="org.argeo.connect.people.ui.commands.EditEntityReferenceWithPosition"
		scope="prototype">
		<property name="peopleService" ref="peopleService" />
	</bean>
	<bean id="org.argeo.connect.people.ui.editEntityReference"
		class="org.argeo.connect.people.ui.commands.EditEntityReference"
		scope="prototype">
		<property name="peopleService" ref="peopleService" />
	</bean>
	<bean id="org.argeo.connect.people.ui.removeEntityReference"
		class="org.argeo.connect.people.ui.commands.RemoveEntityReference"
		scope="prototype">
		<property name="peopleService" ref="peopleService" />
	</bean>
	<bean id="org.argeo.connect.people.ui.deleteEntity" class="org.argeo.connect.people.ui.commands.DeleteEntity"
		scope="prototype">
		<property name="peopleService" ref="peopleService" />
	</bean>


	<!-- <bean id="org.argeo.connect.people.ui.openEntityEditor" class="org.argeo.connect.people.ui.commands.OpenEntityEditor" 
		scope="prototype"> <property name="peopleService" ref="peopleService" /> 
		<property name="peopleUiService" ref="peopleUiService" /> </bean> -->
	<bean id="importPeople"
		class="org.argeo.connect.people.ui.commands.ImportPeopleHandler"
		scope="prototype">
		<property name="repository" ref="repository" />
	</bean>
</beans>
