<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="
		http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="feedView" class="org.argeo.connect.ui.crisis.views.FeedView"
		scope="prototype">
		<property name="session" ref="jcrSession" />
		<property name="linkEditorId" value="org.argeo.connect.ui.crisis.linkEditor" />
	</bean>

	<bean id="featureSourcesView" class="org.argeo.gis.ui.views.FeatureSourcesView"
		scope="prototype">
		<property name="session" ref="jcrSession" />
	</bean>

	<bean id="layersView" class="org.argeo.gis.ui.views.LayersView"
		scope="prototype">
	</bean>

</beans>
