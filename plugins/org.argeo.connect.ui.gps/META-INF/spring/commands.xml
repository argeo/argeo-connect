<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="deleteNodes" class="org.argeo.connect.ui.gps.commands.DeleteNodesExt"
			scope="prototype" />
	
	<bean id="openNewRepoWizard" class="org.argeo.connect.ui.gps.commands.OpenNewRepoWizard"
		scope="prototype">
		<property name="uiJcrServices" ref="uiJcrServices" />
	</bean>

	<bean id="importDirectoryContent"
		class="org.argeo.connect.ui.gps.commands.ImportDirectoryContent"
		scope="prototype" />

	<bean id="addFileFolder" class="org.argeo.connect.ui.gps.commands.AddFileFolder"
		scope="prototype" />

	<bean id="newCleanDataSession" class="org.argeo.connect.ui.gps.commands.NewCleanDataSession"
		scope="prototype">
		<property name="uiJcrServices" ref="uiJcrServices" />
	</bean>
	
	<bean id="removeImportedData" class="org.argeo.connect.ui.gps.commands.RemoveImportedData"
		scope="prototype">
		<property name="uiGisServices" ref="uiGisServices" />
	</bean>
	
	<bean id="openCleanDataEditor" class="org.argeo.connect.ui.gps.commands.OpenCleanDataEditor"
		scope="prototype">
		<property name="uiJcrServices" ref="uiJcrServices" />
	</bean>

	<bean id="openLocalRepoEditor" class="org.argeo.connect.ui.gps.commands.OpenLocalRepoEditor"
		scope="prototype">
	</bean>
</beans>
