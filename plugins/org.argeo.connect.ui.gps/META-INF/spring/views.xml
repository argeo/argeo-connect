<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="
		http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="gpsBrowserView" class="org.argeo.connect.ui.gps.views.GpsBrowserView"
		scope="prototype">
		<property name="jcrSession" ref="jcrSession" />
	</bean>

	<!-- Old one, using generic JCR View, not used for the moment being -->
	<bean id="gpsView" class="org.argeo.connect.ui.gps.views.GpsView"
		scope="prototype">
		<property name="repositoryRegister" ref="repositoryRegister" />
	</bean>

</beans>
