<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- RAP Specific open file command -->
	<bean id="org.argeo.connect.people.ui.specific.openFile" class="org.argeo.eclipse.ui.specific.OpenFile"
		scope="prototype">
		<property name="openFileServiceId"
			value="org.argeo.connect.people.rap.peopleOpenFileService" />
	</bean>
	
	<bean id="org.argeo.connect.people.rap.peopleOpenFileService" class="org.argeo.connect.people.rap.PeopleOpenFileService"
			scope="prototype">
		<property name="repository"
			ref="repository" />
	</bean>
</beans>