<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xmlns:aop="http://www.springframework.org/schema/aop" xmlns:flow="http://www.argeo.org/schema/slc-flow"
	xsi:schemaLocation="
	http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
	http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop-2.5.xsd
	http://www.argeo.org/schema/slc-flow http://www.argeo.org/schema/slc-flow-0.12.xsd">

	<import resource="classpath:org/argeo/slc/core/execution/spring.xml" />

	<flow:flow name="gis/importNaturalEarthData">
		<bean class="org.argeo.slc.postgis.ImportShapefile">
			<property name="targetSrs" value="EPSG:3785" />
			<property name="dbHost" value="air" />
			<property name="dbName" value="connect_mideast" />
			<property name="dbUser" value="argeo" />
			<property name="source"
				value="file:${user.home}/gis/data/providers/naturalearth/1.3.0/10m_cultural/10m_admin_0_countries.shp" />
		</bean>
	</flow:flow>

</beans>