<connect = 'http://www.argeo.org/connect'>

// syndication
[connect:syndFeed] > nt:unstructured, mix:title
- argeo:uri (STRING) m

[connect:syndEntry] > nt:unstructured, mix:title, argeo:links
- argeo:uri (STRING) m
- connect:author (STRING) m *
- connect:publishedDate (DATE) m
- connect:updatedDate (DATE) m
- connect:sourceUri (STRING) m
