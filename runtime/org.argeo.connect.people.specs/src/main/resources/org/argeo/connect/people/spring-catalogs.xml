<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:util="http://www.springframework.org/schema/util" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
	http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
	http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-2.5.xsd">

	<!-- Map of the roles used in the user interface - TODO merge with new role 
		definition -->
	<util:map id="managedRoles">
		<entry>
			<key>
				<util:constant
					static-field="com.mrschilling.manager.MsmConstants.ROLE_CONSULTANT" />
			</key>
			<value>ROLE_CONSULTANT</value>
		</entry>
		<entry>
			<key>
				<util:constant static-field="com.mrschilling.manager.MsmConstants.ROLE_MANAGER" />
			</key>
			<value>ROLE_MANAGER</value>
		</entry>
		<entry>
			<key>
				<util:constant static-field="com.mrschilling.manager.MsmConstants.ROLE_ADMIN" />
			</key>
			<value>ROLE_ADMIN</value>
		</entry>
	</util:map>
</beans>