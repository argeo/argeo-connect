<rss = 'http://www.argeo.org/ns/rss'>

[rss:channel] > nt:unstructured, mix:title, rss:tagable
- rss:uri (STRING) m
- rss:link (STRING)

[rss:item] > nt:unstructured, mix:title, rss:tagable
- rss:link (STRING) 
- rss:pubDate (DATE)
- rss:updatedate (DATE)
+ rss:enclosure (nt:unstructured) *

// 
// TAGABLE
//
[rss:tagable] 
mixin
- rss:category (STRING) *
