<cms = 'http://www.argeo.org/ns/cms'>

[cms:styled]
- cms:style (STRING)
- cms:content (STRING)

[cms:media] > nt:file, mix:title

[cms:section] > nt:hierarchyNode, mix:created, mix:lastModified, mix:title
orderable
+ cms:h (cms:section) *
+ cms:p (cms:styled) *
+ cms:m (cms:media) *

